{
  "name": "ahau-desktop",
  "version": "2.6.2",
  "description": "An application for gathering and sharing whakapapa (family trees, traditional knowledge). Built using scuttlebutt",
  "homepage": "https://gitlab.com/ahau/whakapapa-ora",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron index.js -- --title Āhau",
    "dev": "cross-env NODE_ENV=development electron index.js -- --title Āhau",
    "build:icons": "node build/build-icons.js",
    "pack": "electron-builder --config build/electron-builder.config.js --dir",
    "dist": "electron-builder --config build/electron-builder.config.js",
    "publish": "electron-builder --config build/electron-builder.config.js --publish always",
    "clear": "rm -rf node_modules",
    "win:clear": "rmdir /s /q node_modules"
  },
  "dependencies": {
    "ahau-env": "^3.0.0",
    "boxen": "^5.0.1",
    "chalk": "^4.1.2",
    "electron-updater": "^4.3.8",
    "lodash.merge": "^4.6.2",
    "secret-stack": "^6.4.0",
    "ssb-ahau": "^14.4.0",
    "ssb-ahoy": "^4.1.2",
    "ssb-artefact": "^3.2.1",
    "ssb-backlinks": "^2.1.1",
    "ssb-blobs": "^2.0.0",
    "ssb-config": "^3.4.6",
    "ssb-conn": "^1.0.0",
    "ssb-db": "^20.4.1",
    "ssb-friends": "~4.2.1",
    "ssb-hyper-blobs": "^3.0.0",
    "ssb-invite": "^2.1.7",
    "ssb-lan": "^1.0.1",
    "ssb-master": "^1.0.3",
    "ssb-no-auth": "^1.0.0",
    "ssb-profile": "^5.4.0",
    "ssb-query": "^2.4.5",
    "ssb-recps-guard": "^2.1.0",
    "ssb-replicate": "^1.3.3",
    "ssb-serve-blobs": "^3.0.0",
    "ssb-settings": "^1.1.4",
    "ssb-story": "^2.4.0",
    "ssb-tribes": "^2.7.3",
    "ssb-tribes-registration": "^0.1.0",
    "ssb-unix-socket": "^1.0.0",
    "ssb-whakapapa": "^3.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron-builder": "22.10.5",
    "electron-notarize": "^1.0.0",
    "esm": "^3.2.25",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Āhau Ltd.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ahau/whakapapa-ora.git"
  },
  "license": "AGPL-3.0"
}
