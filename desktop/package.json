{
  "name": "ahau-desktop",
  "version": "0.7.1",
  "description": "An application for gathering and sharing whakapapa (family trees, traditional knowledge). Built using scuttlebutt",
  "homepage": "https://gitlab.com/ahau/whakapapa-ora",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron index.js -- --title Āhau",
    "start:dev": "cross-env NODE_ENV=development electron index.js -- --title Āhau",
    "build:icons": "node build/build-icons.js",
    "pack": "electron-builder --config build/electron-builder.config.js --dir",
    "dist": "electron-builder --config build/electron-builder.config.js"
  },
  "dependencies": {
    "ahau-env": "^1.0.0",
    "secret-stack": "^6.3.1",
    "ssb-ahau": "^1.0.6",
    "ssb-ahoy": "^3.0.0",
    "ssb-artefact": "^2.0.0",
    "ssb-backlinks": "^2.1.1",
    "ssb-blobs": "^1.2.3",
    "ssb-config": "^3.4.5",
    "ssb-conn": "^0.17.0",
    "ssb-db": "^20.3.0",
    "ssb-friends": "^4.2.1",
    "ssb-invite": "^2.1.6",
    "ssb-lan": "^0.2.0",
    "ssb-master": "^1.0.3",
    "ssb-no-auth": "^1.0.0",
    "ssb-profile": "^1.5.2",
    "ssb-query": "^2.4.5",
    "ssb-recps-guard": "^2.1.0",
    "ssb-replicate": "^1.3.2",
    "ssb-serve-blobs": "2.4.0",
    "ssb-story": "^1.0.2",
    "ssb-tribes": "^0.4.0",
    "ssb-unix-socket": "^1.0.0",
    "ssb-whakapapa": "^2.0.2"
  },
  "devDependencies": {
    "@fiahfy/icns-convert": "0.0.8",
    "cross-env": "^7.0.2",
    "electron-builder": "^21.2.0",
    "electron-notarize": "^0.2.1",
    "electron-rebuild": "^1.11.0",
    "esm": "^3.2.25",
    "sharp": "^0.23.4",
    "to-ico": "^1.1.5",
    "typescript": "^3.7.2"
  },
  "author": "Āhau Ltd.",
