{
  "name": "ahau-desktop",
  "version": "2.0.1",
  "description": "An application for gathering and sharing whakapapa (family trees, traditional knowledge). Built using scuttlebutt",
  "homepage": "https://gitlab.com/ahau/whakapapa-ora",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron index.js -- --title Āhau",
    "dev": "cross-env NODE_ENV=development electron index.js -- --title Āhau",
    "build:icons": "node build/build-icons.js",
    "pack": "electron-builder --config build/electron-builder.config.js --dir",
    "dist": "electron-builder --config build/electron-builder.config.js",
    "publish": "electron-builder --config build/electron-builder.config.js --publish always",
    "clear": "rm -rf node_modules"
  },
  "dependencies": {
    "ahau-env": "^3.0.0",
    "electron-updater": "^4.3.8",
    "lodash.merge": "^4.6.2",
    "secret-stack": "^6.4.0",
    "ssb-ahau": "^12.0.1",
    "ssb-ahoy": "^4.1.2",
    "ssb-artefact": "^3.2.1",
    "ssb-backlinks": "^2.1.1",
    "ssb-blobs": "^2.0.0",
    "ssb-config": "^3.4.5",
    "ssb-conn": "^1.0.0",
    "ssb-db": "^20.3.0",
    "ssb-friends": "~4.2.1",
    "ssb-hyper-blobs": "^2.1.1",
    "ssb-invite": "^2.1.7",
    "ssb-lan": "^1.0.0",
    "ssb-master": "^1.0.3",
    "ssb-no-auth": "^1.0.0",
    "ssb-profile": "^3.1.0",
    "ssb-query": "^2.4.5",
    "ssb-recps-guard": "^2.1.0",
    "ssb-replicate": "^1.3.3",
    "ssb-serve-blobs": "^2.5.0",
    "ssb-settings": "^1.1.4",
    "ssb-story": "^2.2.3",
    "ssb-tribes": "^2.1.0",
    "ssb-unix-socket": "^1.0.0",
    "ssb-whakapapa": "^2.1.0"
  },
  "devDependencies": {
    "@fiahfy/icns-convert": "0.0.8",
    "cross-env": "^7.0.2",
    "electron-builder": "22.10.5",
    "electron-notarize": "^1.0.0",
    "esm": "^3.2.25",
    "sharp": "^0.23.4",
    "to-ico": "^1.1.5",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Āhau Ltd.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ahau/whakapapa-ora.git"
  },
  "license": "AGPL-3.0"
}
