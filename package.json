    "pack": "electron-builder --config build/electron-builder.config.js --dir",
    "dist": "electron-builder --config build/electron-builder.config.js",
    "publish": "npm run build:UI && npm run dist",
    "start": "electron index.js -- --title Āhau",
    "storybook": "start-storybook -p 6001",
    "lint": "vue-cli-service lint",
    "test:js": "tape -r 'esm' 'src/**/*.test.js' | tap-spec",
    "test": "npm run test:js && npm run lint"
  },
