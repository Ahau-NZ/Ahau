{
  "name": "whakapapa-ora",
  "version": "0.1.2",
  "scripts": {
    "postinstall": "npm run rebuild",
    "rebuild": "electron-builder install-app-deps",
    "build": "vue-cli-service build",
    "icon": "./build/build-icon",
    "start": "electron index.js -- --title whakapapa-ora",
    "dist": "electron-builder",
    "publish": "npm run build && npm run dist",
    "serve": "vue-cli-service serve",
    "start:dev": "NODE_ENV=development electron index.js -- --title whakapapa-ora",
    "start:gql": "nodemon ./graphql/test/server.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apollo-server": "^2.9.3",
    "apollo-server-express": "^2.9.6",
    "apollo-upload-client": "^10.0.1",
    "casual": "^1.6.2",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "d3": "^5.12.0",
    "express": "^4.17.1",
    "graphql-tag": "^2.9.0",
    "graphql-upload": "^8.0.7",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "pull-file": "^1.1.0",
    "pull-paramap": "^1.2.2",
    "pull-stream": "^3.6.14",
    "secret-stack": "^6.3.0",
    "ssb-ahoy": "github:ssbc/ssb-ahoy#secret-stack",
    "ssb-backlinks": "^0.7.3",
    "ssb-blobs": "^1.2.2",
    "ssb-client": "^4.7.8",
    "ssb-config": "^3.4.3",
    "ssb-conn": "^0.11.2",
    "ssb-db": "^19.3.1",
    "ssb-friends": "^4.1.4",
    "ssb-invite": "^2.1.4",
    "ssb-lan": "^0.1.3",
    "ssb-legacy-conn": "^1.0.25",
    "ssb-master": "^1.0.3",
    "ssb-no-auth": "^1.0.0",
    "ssb-private": "^0.2.3",
    "ssb-profile": "^0.3.1",
    "ssb-promiscuous": "^1.1.0",
    "ssb-query": "^2.4.3",
    "ssb-replicate": "^1.3.0",
    "ssb-serve-blobs": "^2.1.0",
    "ssb-unix-socket": "^1.0.0",
    "stream-to-pull-stream": "^1.7.3",
    "typeface-roboto": "^0.0.75",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-cli-plugin-apollo": "^0.21.0",
    "vue-context": "^4.1.1",
    "vue-router": "^3.0.3",
    "vuetify": "^2.1.0",
    "vuetify-image-input": "^19.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@fiahfy/icns-convert": "0.0.6",
    "@mdi/font": "^4.5.95",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "electron-builder": "^21.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "sharp": "^0.23.1",
    "to-ico": "^1.1.5",
    "vue-cli-plugin-vuetify": "^1.0.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "build": {
    "appId": "org.ahau.whakapapa-ora",
    "directories": {
      "output": "dist/installers"
    },
    "linux": {
      "category": "Network",
      "target": "AppImage"
    },
    "appImage": {
      "artifactName": "${name}-Linux-${version}-${arch}.${ext}"
    },
    "dmg": {
      "artifactName": "${name}-Mac-${version}.${ext}",
      "icon": "build/dmg-icon.icns"
    },
    "win": {
      "publisherName": "Ahau"
    },
    "nsis": {
      "artifactName": "${name}-Windows-${version}.${ext}",
      "installerIcon": "build/setup-icon.ico"
    },
    "files": [
      "**/*",
      "!src/*",
      "!public/*",
      "!dist/installers",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ]
  }
}
