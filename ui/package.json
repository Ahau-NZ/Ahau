{
  "name": "ahau-ui",
  "version": "2.28.0",
  "description": "An application for gathering and sharing whakapapa (family trees, traditional knowledge). Built using scuttlebutt",
  "author": "Ä€hau Ltd.",
  "scripts": {
    "postinstall": "patch-package && npm run fix:sass",
    "fix:sass": "npx sass-migrator division node_modules/vuetify/**/*.s*ss",
    "dev": "vite --port 3000",
    "build": "vite build",
    "build:desktop": "vite build --outDir ../desktop/dist",
    "build:mobile": "cross-env VITE_APP_PLATFORM=cordova vite build --outDir ../mobile/www",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.mjs,.vue --ignore-path .gitignore --fix src",
    "test:js": "cross-env NODE_ENV=development tape 'src/**/*.test.*js' | tap-arc",
    "test:no-only": "if grep -r --exclude-dir=node_modules --exclude-dir=.git --color 'test\\.only' ; then exit 1; fi",
    "test": "npm run test:js && npm run test:no-only && npm run lint",
    "clear": "rm -rf node_modules",
    "win:clear": "rmdir /s /q node_modules",
    "build-fragment": "NODE_ENV=development ./bin/build-fragment.js"
  },
  "browserslist": [
    "electron 18.0.1"
  ],
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "ahau-env": "^3.0.0",
    "ahau-graphql-client": "^2.0.0",
    "animate.css": "^4.1.1",
    "axios": "^1.5.1",
    "capture-frame": "^4.0.0",
    "colorette": "^2.0.20",
    "d3": "^7.8.5",
    "edtf": "^4.4.2",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lodash-es": "^4.17.21",
    "mixpanel-browser": "^2.47.0",
    "pile-sort": "^1.0.2",
    "pull-paramap": "^1.2.2",
    "pull-pushable": "^2.2.0",
    "pull-stream": "^3.7.0",
    "readable-stream": "^3.6.2",
    "render-media": "^4.1.0",
    "ssb-config": "^3.4.6",
    "ssb-hyper-blobs": "^3.0.0",
    "ssb-uri2": "^1.9.0",
    "uuid": "^3.4.0",
    "vue": "^2.7.14",
    "vue-apollo": "^3.1.1",
    "vue-context": "^5.0.0",
    "vue-gallery": "^2.0.5",
    "vue-i18n": "^8.28.2",
    "vue-router": "^3.6.5",
    "vue-rx": "^6.2.0",
    "vue-zoomer": "^0.3.10",
    "vuejs-clipper": "^4.1.0",
    "vuetify": "2.3.20",
    "vuetify-media-player": "^0.8.1",
    "vuex": "^3.6.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.3",
    "@rushstack/eslint-patch": "^1.5.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "node-stdlib-browser": "^1.2.0",
    "patch-package": "^8.0.0",
    "sass": "^1.68.0",
    "standard": "^17.1.0",
    "tap-arc": "^1.2.2",
    "tape": "^5.7.2",
    "typeface-roboto": "^1.1.13",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-inspector": "^0.5.1",
    "vite": "^4.4.9"
  },
