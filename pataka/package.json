{
  "name": "ahau-pataka",
  "version": "0.0.1",
  "description": "A pub application for the Ahau ecosystem. Built using scuttlebutt",
  "homepage": "https://gitlab.com/ahau/whakapapa-ora",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && npm run ui:setup && npm run server:setup",
    "start": "electron index.js -- --title Pātaka",
    "electron:dev": "cross-env NODE_ENV=development electron index.js -- --title Pātaka",
    "start:dev": "npm run ui:dev 1>&2 | npm run electron:dev",
    "ui:setup": "cd ui && npm install",
    "server:setup": "cd ssb-pataka && npm install",
    "ui:dev": "cd ui && npm run dev",
    "build:ui": "cd ui && npm run build",
    "build:icons": "node build/build-icons.js",
    "pack": "electron-builder --config build/electron-builder.config.js --dir",
    "dist": "electron-builder --config build/electron-builder.config.js",
    "lint": "cd ui && npm run lint"
  },
  "dependencies": {
    "secret-stack": "^6.3.1",
    "ssb-ahoy": "^3.0.0",
    "ssb-artefact": "^2.0.0",
    "ssb-backlinks": "^2.1.1",
    "ssb-blobs": "^1.2.3",
    "ssb-config": "^3.4.4",
    "ssb-conn": "^0.17.0",
    "ssb-db": "^20.3.0",
    "ssb-friends": "^4.2.0",
    "ssb-invite": "^2.1.6",
    "ssb-lan": "^0.2.0",
    "ssb-master": "^1.0.3",
    "ssb-no-auth": "^1.0.0",
    "ssb-pataka": "file:./ssb-pataka",
    "ssb-profile": "^1.5.1",
    "ssb-query": "^2.4.5",
    "ssb-recps-guard": "^2.1.0",
    "ssb-replicate": "^1.3.2",
    "ssb-serve-blobs": "2.4.0",
    "ssb-story": "^1.0.2",
    "ssb-tribes": "^0.2.4",
    "ssb-unix-socket": "^1.0.0",
    "ssb-whakapapa": "^2.0.2"
  },
  "devDependencies": {
    "@fiahfy/icns-convert": "0.0.8",
    "cross-env": "^7.0.2",
    "electron-builder": "^22.7.0",
    "electron-notarize": "^1.0.0",
    "electron-rebuild": "^1.11.0",
    "esm": "^3.2.25",
    "sharp": "^0.25.4",
    "to-ico": "^1.1.5",
    "typescript": "^3.9.6"
  },
  "author": "Āhau Ltd.",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/ahau/whakapapa-ora.git"
  },
  "license": "AGPL-3.0"
}
